public class Bill - Copy{
  
  public static int coogie() {
    int bird = 0;
    for (int l=0; l <100000; l++) {
      
      for (int m = 0; m < 100; m++) {
        bird += (l + m + 2);
      }
      
    }
    return bird;
  }
  
  
  public static int noogie() {
    int bird = 0;
    for (int l=0; l <1000; l++) {
      for (int m = 0; m < 100; m++) {
        bird += (l + m);
      }
    }
    return bird;
  }
  
  
  public static void main(String[] args) {
    System.out.println(coogie());
    
    
    
  }
  
}
